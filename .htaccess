<IfModule mod_rewrite.c>

 # allow_override On
 # mod_rewrite is installed
 
 RewriteEngine on
 RewriteCond %{REQUEST_FILENAME} !-f
 RewriteCond %{REQUEST_FILENAME} !-d
 RewriteRule ^(.*)$ index.php/$1 [L]
 
</IfModule>